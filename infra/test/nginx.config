server {
		listen 80;
		server_name ABCDEFG.socket.lfdev.io;
		
    # Block specific User-Agents
    if ($http_user_agent ~* (Googlebot|NaverBot|bingbot|AhrefsBot|SemrushBot|MJ12bot|DotBot|Baiduspider)) {
        return 403;
    }
		
		location / {
				
				proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
				proxy_set_header Host $host;
				proxy_set_header X-Real-IP $remote_addr;
				proxy_set_header X-Forwarded-Proto $scheme;
				
				proxy_pass http://localhost:8500;
				
				# enable WebSockets
				proxy_http_version 1.1;
				proxy_set_header Upgrade $http_upgrade;
				proxy_set_header Connection "upgrade";
		}
		    
    location = /robots.txt {
        alias /var/www/robots.txt;  # Absolute path to the specific file
        access_log off;  # Optional: Disable logging for robots.txt requests
    }
}
