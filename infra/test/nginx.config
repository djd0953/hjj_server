server {
		listen 80;
		server_name ABCDEFG.lfdev.io;
		
    # Block specific User-Agents
    if ($http_user_agent ~* (Googlebot|NaverBot|bingbot|AhrefsBot|SemrushBot|MJ12bot|DotBot|Baiduspider)) {
        return 403;
    }

		location / {
				proxy_pass http://localhost:3000;
		}
    
    location = /robots.txt {
        alias /var/www/robots.txt;  # Absolute path to the specific file
        access_log off;  # Optional: Disable logging for robots.txt requests
    }
}
server {
		listen 80;
		server_name ABCDEFG.doc.lfdev.io;
		
    root /var/build; # Path to your React build directory
    index index.html;
    
    # Block specific User-Agents
    if ($http_user_agent ~* (Googlebot|NaverBot|bingbot|AhrefsBot|SemrushBot|MJ12bot|DotBot|Baiduspider)) {
        return 403;
    }

    location / {
        try_files $uri /index.html;
    }
    
    location = /robots.txt {
        alias /var/www/robots.txt;  # Absolute path to the specific file
        access_log off;  # Optional: Disable logging for robots.txt requests
    }
}
